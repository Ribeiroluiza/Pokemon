// conex√µes
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// Tabelas Banco de dados 

model pokemon {
  id   String @id @default(uuid())
  nometreinador String
  idadetreinador Int
  generotreinador String
  tipopokemon String
  nomepokemon String
  cidadepokemon String


}
